<div class="followers-list" style="margin-top: 20px; border-bottom: 1px solid #eee;">
  <p style="font-weight: bold;">
    <i class="fa fa-users"></i>
    已关注(<%= count %>)
  </p>
  <div class="user-list">
    <% userList.forEach(user => { %>
      <a href="/profile/<% user.userName %>" class="user-name" style="column-rule: olivedrab;">
        <img src="<%= user.picture %>" alt="" style="width: 40px; height: 40px; border-radius: 50%;">
        <span><%= user.nickName %></span>
      </a>
    <% }) %>
  </div>
</div>