<div class="fans-list" style="margin-top: 20px; border-bottom: 1px solid #eee;">
  <p style="font-weight: bold;">
    <i class="fa fa-users"></i>
    粉丝(<%= count %>)
  </p>
  <div class="user-list" style="display: flex;">
    <% userList.forEach(user => { %>
      <a href="/profile/<% user.userName %>" class="user-name" style="display: flex; flex-direction: column; align-items: center; width: 50px;">
        <img src="<%= user.picture %>" alt="" style="width: 40px; height: 40px; border-radius: 50%;">
        <span style="text-align: center;"><%= user.nickName %></span>
      </a>
    <% }) %>
  </div>
</div>